<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<bean id="tuling" class="cn.nzcong.robot.service.tuling.impl.ChartServiceImpl" />
	<bean name="/robotservice/tuling"
		class="org.springframework.remoting.caucho.HessianServiceExporter">
		<property name="service" ref="tuling" />
		<property name="serviceInterface" value="cn.nzcong.robot.service.RobotService" />
	</bean>

</beans>
